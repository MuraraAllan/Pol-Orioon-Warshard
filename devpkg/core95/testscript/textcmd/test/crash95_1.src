Use uo;
Use os;

Program TextCMD_Pol095LinuxCrash(who)
  Var queue:={};       // Ok
  Var spawntime;       // UnInit
  Var spawnrunes:={1}; // Ok

  SysLog("queue     ="+queue);
  SysLog("spawntime ="+spawntime);
  SysLog("spawnrunes="+spawnrunes);

  queue[spawntime]:=spawnrunes; // And... Crash

  SysLog("queue[spawntime]="+queue[spawntime]);
  SysLog("No Crash   :o(");
EndProgram