      8: info(who, trg.GetCharacter(4).serial, trg);
      9: info(who, trg.GetCharacter(5).serial, trg);
     ///10: SendSysMessage(who,"Desativado! Use '.jailar'",3,33); ///editjail(who, trg);
     11: ban(who, trg.name);
    endcase
