function banir(who, conta);

var vcaceita:=SendDialogGump(who, xxxx, dddd);

if(!vcaceita)
SendSysMessage(who, "Cancelado.",9,33);
return;
endif

SendSysMessage(who, "Conta " +conta.name+ " banida!",3,33);
syslog("[JAIL]: Conta " + conta.name + " foi banida. Pelo Login Staff " +who.acct.name);
      foreach char in EnumerateOnlineCharacters();
         if(char.acctname == conta)
           SendSysMessage(char, "Voce foi banido do servidor.",3,33);
           DisconnectClient(char);
           ///break;
         endif
      endforeach
       Detach();
       conta.ban();
       conta.disable();
endfunction

function infoplayer(who,conta,char);
