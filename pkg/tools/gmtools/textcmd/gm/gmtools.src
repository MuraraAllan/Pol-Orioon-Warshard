    if(testdata[20] == error)
      testdata[20] := "";
    endif
    testdata[21] := trg.GetCharacter(4).name;
    if(testdata[21] == error)
      testdata[21] := "";
    endif
    testdata[22] := trg.GetCharacter(5).name;
    if(testdata[22] == error)
      testdata[22] := "";
    endif
